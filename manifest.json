{
    "manifest_version": 2,
    "name": "landsoflords community extension",
    "version": "0.1",

    "description": "add multiple UI improvements for landsoflords",

    "permissions":
    [
        "*://*.landsoflords.com/*",
        "storage"
    ],

    "content_scripts": 
    [
        {
            "matches": ["*://*.landsoflords.com/*"],
            "js": ["utils.js", "custom_game.js", "content.js"]
        }
    ],

    "options_ui": 
    {
        "page": "config/config.html"
    }

}